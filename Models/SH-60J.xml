				<property>position/altitude-agl-ft</property>
				<factor>-40</factor>
				<offset-m>2000</offset-m>
			</particles-per-sec>
		</counter>

		<align>billboard</align>

		<particle>
			<start>
				<color>
					<red>
						<value>0.8</value>
					</red>
					<green>
						<value>0.8</value>
					</green>
					<blue>
						<value>1.0</value>
					</blue>
					<alpha>
						<property>rotors/main/rpm</property>
						<factor>0.00004</factor>
						<max>0.001</max>
					</alpha>
				</color>
				<size>
					<value>0.05</value>
				</size>
			</start>

			<end>
				<color>
					<red>
						<value>0.8</value>
					</red>
					<green>
						<value>0.8</value>
					</green>
					<blue>
						<value>1.0</value>
					</blue>
					<alpha>
						<property>rotors/main/rpm</property>
						<factor>0.0002</factor>
						<max>0.004</max>
					</alpha>
				</color>
				<size>
					<value>3.0</value>
				</size>
			</end>

			<life-sec>
				<value>1.5</value>
			</life-sec>
			<mass-kg>0.001</mass-kg>
			<radius-m>0.01</radius-m>
		</particle>

		<program>
			<fluid>water</fluid>
			<gravity>false</gravity>
			<wind>false</wind>
		</program>
	</particlesystem>

