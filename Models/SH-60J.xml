	   <interpolation>
		<entry> <ind> 0   </ind> <dep>   0 </dep> </entry>
		<entry> <ind> 0.5 </ind> <dep>   0 </dep> </entry>
		<entry> <ind> 1   </ind> <dep> 175 </dep> </entry>
	   </interpolation>
    <axis>
      <x1-m>  7.871 </x1-m>
      <y1-m> -0.228 </y1-m>
      <z1-m> -1.039 </z1-m>
      <x2-m>  7.968 </x2-m>
      <y2-m> -0.204 </y2-m>
      <z2-m> -1.491 </z2-m>
    </axis>
  </animation>


